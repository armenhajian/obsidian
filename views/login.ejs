<body>

<div style="max-width:500px;margin: 0 auto;">
    <form action="/login" method="post">
        <div class="form-group">

            <div class="col-sm-10">
                <input type="text" name="username" class="form-control" id="i5ps" placeholder="Username">
            </div>
            <div class="col-sm-10">
                <input type="password" name="password" class="form-control" id="i5ps" placeholder="Password">
            </div>
            <div class="col-sm-10">
                <input type="submit" class="form-control" id="i5ps">
            </div>
        </div>
    </form>
</div>
